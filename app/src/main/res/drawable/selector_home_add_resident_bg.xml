<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    

    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/dp_8" />
            <solid android:color="@color/title_center_color" />
            <stroke android:width="@dimen/dp_1.5" android:color="@color/title_stroke_color" />
        </shape>

    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/dp_8" />
            <solid android:color="@color/title_end_color" />
            <stroke android:width="@dimen/dp_1.5" android:color="@color/title_stroke_color" />
        </shape>
    </item>
</selector>